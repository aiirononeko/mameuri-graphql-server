# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
}

type Query {
  businessUsers: [BusinessUser!]!
}

input newBusinessUsers {
  email: String!
  password: String!
}

type Mutation {
  createBusinessUser(input: newBusinessUsers!): BusinessUser!
}

type BusinessUser {
  id: ID!
  email: String!
  password: String!
}
