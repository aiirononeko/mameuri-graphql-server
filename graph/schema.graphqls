type BusinessUser {
  id: ID!
  email: String!
  password: String!
}

type Query {
  allBusinessUsers(orderBy: OrderBy, first: Int!, skip: Int!): [BusinessUser!]!
}

enum OrderBy {
  createdAt_ASC,
  createdAt_DESC
}

type Mutation {
  createBusinessUser(email: String!, password: String!): BusinessUser!
  updateBusinessUser(id: String, password: String!): BusinessUser!
  deleteBusinessUser(id: String, password: String!): BusinessUser!
}
